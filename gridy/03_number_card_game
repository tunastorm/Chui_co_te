from random import randint 

def make_line(stop, maximum):
    line = ''
    for _ in range(stop):
        line += '{} '.format(randint(1,maximum))
    line.rstrip(' ')
    return line

N, M = randint(1, 10), randint(1, 10)
print(N, M)

line_list = []
first_line = f'{N} {M}'
for i in range(N):
    line = make_line(M, 10000)
    line_list.append(line)

result = 0
''' í’€ì´ 1
